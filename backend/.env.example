# Database Configuration
DATABASE_URL="file:./prisma/dashboard.db"

# Scheduler Configuration
# Set to 'true' to enable automatic periodic data imports
SCHEDULER_ENABLED=false

# Cron expression for scheduled imports
# Examples:
#   */5 * * * *  = Every 5 minutes (testing)
#   0 * * * *    = Every hour
#   0 2 * * *    = Daily at 2 AM (recommended)
#   0 0 * * 0    = Weekly on Sunday
#   0 0 1 * *    = Monthly on the 1st
CRON_EXPRESSION=0 2 * * *

# Path to Excel file for scheduled imports
# Can be absolute or relative to backend directory
DATA_FILE_PATH=uploads/ai_coding_agent_dashboard_data.xlsx

# Server Configuration
# Host and port the Express server binds to.
HOST=127.0.0.1
PORT=8080
